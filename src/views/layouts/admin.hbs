<table>
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Email</th>
            <th>Rango</th>
            <th>.</th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
        <tr>
            <td>{{this.first_name}} {{this.last_name}}</td>
            <td>{{this.email}}</td>
            <td>{{this.role}}</td>
            <td>
                <form action="api/users/admin/{{this._id}}" method="POST">
                    <button type="submit">Eliminar usuario</button>
                </form>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>